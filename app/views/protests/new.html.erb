<h1>Create a  protest</h1>

<%= form_for(@protest, html: {multipart: true}) do |f| %>
  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %>
  <div>

  <div class="field">
    <%= f.label :description %>
    <%= f.text_field :description %>
  <div>

  <div class="field">
    <%= f.label :location %>
    <%= f.text_field :location %>
  <div>

  <div class="field">
    <%= f.label :time %>
    <%= f.datetime_local_field :time %>
  <div>

  <div class="field">
    <%= f.label :image %>
    <%= f.file_field :image %>
  <div>

  <div class="field">
    <%= f.submit "Create" %>
  <div>
<% end %>
